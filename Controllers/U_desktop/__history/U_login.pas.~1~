unit U_login;

interface

uses
  Windows, Messages, SysUtils, Variants, Classes, Graphics,
  Controls, Forms, uniGUITypes, uniGUIAbstractClasses,
  uniGUIClasses, uniGUIRegClasses, uniGUIForm, uniLabel, uniEdit,
  uniGUIBaseClasses, uniPanel;

type
  TUniLoginForm1 = class(TUniLoginForm)
    UniPanel2: TUniPanel;
    UniPanel1: TUniPanel;
    UniPanel3: TUniPanel;
    UniPanel4: TUniPanel;
    UniPanel5: TUniPanel;
    UniEdit1: TUniEdit;
    UniLabel1: TUniLabel;
    UniLabel2: TUniLabel;
    UniPanel6: TUniPanel;
    UniEdit2: TUniEdit;
    UniLabel3: TUniLabel;
    UniPanel7: TUniPanel;
    UniEdit3: TUniEdit;
    UniLabel4: TUniLabel;
    UniPanel8: TUniPanel;
  private
    { Private declarations }
  public
    { Public declarations }
  end;

function UniLoginForm1: TUniLoginForm1;

implementation

{$R *.dfm}

uses
  uniGUIVars, MainModule, uniGUIApplication;

function UniLoginForm1: TUniLoginForm1;
begin
  Result := TUniLoginForm1(UniMainModule.GetFormInstance(TUniLoginForm1));
end;

initialization
  RegisterAppFormClass(TUniLoginForm1);

end.
